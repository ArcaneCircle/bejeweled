{
  "name": "bejeweled",
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "build": "vite build && jszip-cli -c jszip.config.json",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "dev": "vite --port 3333",
    "webxdc-dev": "webxdc-dev run http://localhost:3333/",
    "webxdc-xdc": "webxdc-dev run dist/package.xdc",
    "old-dev": "concurrently \"pnpm webxdc-dev\" \"pnpm vite-dev\""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "phaser": "^3.55.2",
    "phaser3-rex-plugins": "^1.1.71"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@ffflorian/jszip-cli": "^3.1.6",
    "@types/node": "^17.0.45",
    "concurrently": "^7.2.2",
    "eruda": "^2.4.1",
    "eslint": "^8.18.0",
    "jsdom": "^19.0.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.7.2",
    "vite": "^2.9.12",
    "vitest": "^0.12.10",
    "webxdc-dev": "^0.15.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "eslintIgnore": [
    "webxdc.js",
    "webxdc.d.ts"
  ]
}
